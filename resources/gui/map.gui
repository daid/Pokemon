            [TYPE1] {
                type: label
                caption: NORMAL
                size: 64, 8
                position: 8, 16
                text_alignment: left
            }
            {
                type: label
                caption: TYPE2/
                size: 64, 8
                position: 0, 24
                text_alignment: left
            }
            [TYPE2] {
                type: label
                caption: NORMAL
                size: 64, 8
                position: 8, 32
                text_alignment: left
            }
        }
        [B2] {
            type: panel
            visible: false
            position: 0, 64
            size: 160, 80
            
            [MOVE1] {
                type: label
                caption: MOVE1
                size: 136, 8
                position: 8, 8
                text_alignment: left
            }
            [MOVE1_PP] {
                type: label
                caption: PP 11/12
                size: 136, 8
                position: 8, 16
                text_alignment: right
            }
            [MOVE2] {
                type: label
                caption: MOVE2
                size: 136, 8
                position: 8, 24
                text_alignment: left
            }
            [MOVE2_PP] {
                type: label
                caption: PP 11/12
                size: 136, 8
                position: 8, 32
                text_alignment: right
            }
            [MOVE3] {
                type: label
                caption: MOVE3
                size: 136, 8
                position: 8, 40
                text_alignment: left
            }
            [MOVE3_PP] {
                type: label
                caption: PP 11/12
                size: 136, 8
                position: 8, 48
                text_alignment: right
            }
            [MOVE4] {
                type: label
                caption: MOVE4
                size: 136, 8
