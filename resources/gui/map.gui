                text_alignment: left
            }
            [MOVE4_PP] {
                type: label
                caption: PP 11/12
                size: 136, 8
                position: 8, 64
                text_alignment: right
            }
        }
    }
    
    [POKEDEX] {
        stretch: true
        visible: false

        {
            type: panel
            stretch: true
            margin: -8
            order: -10
        }
        
        {
            type: label
            caption: CONTENTS
            text_alignment: left
            size: 80, 8
            position: 8, 8
        }
        
        [ENTRIES] {
            type: label
            text_alignment: topleft
            size: 80, 128
            position: 8, 16
        }

        [CURSOR] {
            type: label
            text_alignment: bottomleft
            position: 0, 0
            margin: 0, 16
            size: 8, 16
            caption: >
        }
    }

    [POKEDEX_ENTRY] {
        stretch: true
        #visible: false

        {
            type: panel
            stretch: true
            margin: -8
            order: -10
        }
        
        [IMAGE] {
            type: image
            size: 56, 56
            alignment: topleft
            margin: 8, 0
            texture: gfx/front/NIDORAN_M.png
        }

        [NO] {
            type: label
            size: 56, 8
            alignment: topleft
            position: 8, 56
            caption: #NO.123
        }

        [NAME] {
            type: label
            size: 80, 8
            alignment: topright
            text_alignment: left
            caption: NAME123456
            position: 0, 8
        }

        [SPECIES] {
            type: label
            size: 80, 8
            alignment: topright
            text_alignment: left
            caption: SPECIES
            position: 0, 24
        }

        [HEIGHT] {
            type: label
            size: 80, 8
            alignment: topright
            text_alignment: left
            caption: HT  2#INCH12#FEET
            position: 0, 40
        }
        [WEIGHT] {
            type: label
            size: 80, 8
            alignment: topright
            text_alignment: left
            caption: WT  20lb
            position: 0, 56
        }

        [INFO] {
            type: label
            size: 144, 64
            alignment: bottomleft
            text_alignment: topleft
            position: 8, 0
        }
    }
    
    [MESSAGE_BOX] {
        type: panel
        alignment: bottom
        fill_width: true
        size: 100, 48
        visible: false
        
        [MESSAGE_BOX_LABEL] {
            type: label
            stretch: true
            text_alignment: topleft
            margin: 8, 8, 16, 8
        }
        [MESSAGE_BOX_MORE_INDICATOR] {
            type: label
            stretch: true
            text_alignment: bottomright
            margin: 8
            caption: #DOWN
            visible: false
        }
    }
    
    [CONFIRM_BOX] {
        type: panel
        alignment: bottomright
        position: 0, -48
        size: 48, 40
        order: 100
        visible: false

        {
            type: label
            stretch: true
            text_alignment: topleft
            margin: 16, 8
            caption: YES\
                \
                NO
        }
        
        [CONFIRM_CURSOR] {
            type: label
            stretch: true
            text_alignment: topleft
            margin: 8
            caption: >
        }
    }

    [CHOICE_MENU] {
        type: panel
        alignment: bottomleft
        position: 8, -48
        size: 144, 80
        order: 100
        visible: false

        [LABEL] {
            type: label
            stretch: true
            text_alignment: topleft
            margin: 16, 8
            caption: 1\
                2\
                3\
                4\
                5\
                6\
                7\
                8
        }
        
        [CURSOR] {
            type: label
            stretch: true
            text_alignment: topleft
            margin: 8
            size: 8, 8
            caption: >
        }
    }
}

[@SELECT_POKEMON_ROW] {
    fill_width: true
    size: 100, 16
    [ICON] {
        type: image
        texture: gfx/sprite/ball.png
        position: 8, 0
        size: 16, 16
    }
    [NAME] {
        type: label
        position: 24, 0
        size: 80, 8
        caption: 1234512345
        text_alignment: topleft
    }
    {
        type: label
        position: 32, 8
        size: 16, 8
        caption: #HP
        text_alignment: topleft
    }
    [HP] {
        type: progressbar
        alignment: topleft
        margin: 46, 8
        size: 50, 8
    }
    [LEVEL] {
        type: label
        alignment: topleft
        text_alignment: left
        margin: 104, 0
        size: 0, 8
        caption: #LVL12
    }
    [HP_LABEL] {
        type: label
        alignment: topleft
        text_alignment: left
        margin: 104, 8
        size: 0, 8
        caption: 123/123
    }
}