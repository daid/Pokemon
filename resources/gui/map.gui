                size: 64, 8
                position: 8, 16
                text_alignment: right
            }
            {
                type: label
                caption: DEFENSE
                size: 64, 8
                position: 8, 24
                text_alignment: left
            }
            [DEFENSE] {
                type: label
                caption: 1234
                size: 64, 8
                position: 8, 32
                text_alignment: right
            }
            {
                type: label
                caption: SPEED
                size: 64, 8
                position: 8, 40
                text_alignment: left
            }
            [SPEED] {
                type: label
                caption: 1234
                size: 64, 8
                position: 8, 48
                text_alignment: right
            }
            {
                type: label
                caption: SPECIAL
                size: 64, 8
                position: 8, 56
                text_alignment: left
            }
            [SPECIAL] {
                type: label
                caption: 1234
                size: 64, 8
                position: 8, 64
                text_alignment: right
            }
        }
        [A3] {
            type: panel
            visible: false
            alignment: topright
            margin: 0, 0
            theme_data: battle_right_panel
            position: 0, 64
            size: 80, 80

            {
                type: label
                caption: TYPE1/
                size: 64, 8
                position: 0, 8
                text_alignment: left
