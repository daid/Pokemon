[GUI] {
    stretch: true

    [SELECT_POKEMON] {
        visible: false
        fill_width: true
        size: 100, 96

        {
            type: panel
            stretch: true
            margin: -8
            order: -10
        }

        [CURSOR] {
            type: label
            text_alignment: bottomleft
            position: 0, 0
            size: 8, 16
            caption: >
        }
        
        {
            stretch: true
            layout: vertical
            [ROW0] {
                @ref: SELECT_POKEMON_ROW
            }
            [ROW1] {
                @ref: SELECT_POKEMON_ROW
            }
            [ROW2] {
                @ref: SELECT_POKEMON_ROW
            }
            [ROW3] {
                @ref: SELECT_POKEMON_ROW
            }
            [ROW4] {
                @ref: SELECT_POKEMON_ROW
            }
            [ROW5] {
                @ref: SELECT_POKEMON_ROW
            }
        }
    }
    
    [STATS_POKEMON] {
        stretch: true
        visible: false

        {
            type: panel
            stretch: true
            margin: -8
            order: -10
        }
        
        [IMAGE] {
            type: image
            size: 56, 56
            alignment: topleft
            margin: 8, 0
            texture: gfx/front/NIDORAN_M.png
        }

        [NO] {
            type: label
            size: 56, 8
            alignment: topleft
            position: 8, 56
            caption: #NO.123
        }
        
        {
            type: panel
            alignment: topright
            margin: 0, 0
            theme_data: battle_right_panel
            size: 96, 64

            [NAME] {
                type: label
                size: 80, 8
                text_alignment: left
                caption: NAME123456
                position: 8, 8
            }
            
            [A1] {
                stretch: true
                visible: false
                [LVL] {
                    type: label
                    caption: #LVL12
                    size: 40, 8
                    text_alignment: left
                    position: 48, 16
                }
                {
                    type: label
